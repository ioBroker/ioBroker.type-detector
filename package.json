{
  "name": "iobroker.type-detector",
  "version": "4.1.0",
  "description": "Detects devices in ioBroker for Material, Google home, Homekit, ...",
  "author": {
    "name": "bluefox",
    "email": "dogafox@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ioBroker/ioBroker.type-detector",
